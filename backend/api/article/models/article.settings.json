{
  "info": {
    "name": "article"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "titre": {
      "type": "string"
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "logo": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "description": {
      "type": "text"
    },
    "lien": {
      "type": "string"
    },
    "etat_publi": {
      "type": "enumeration",
      "enum": [
        "brouillon",
        "public",
        "archive"
      ]
    }
  },
  "connection": "default",
  "collectionName": "articles"
}
